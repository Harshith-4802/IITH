#Exercise 1.3
from numpy import random
num_trials = 10000
samples = random.rand(num_trials)
output = ""
for s in samples:
	if(s < 0.0620559983):
		output += 'a'
	elif (s < 0.062055998 + 0.4690159091):
		output += 'b'
	elif (s < 0.062055998 + 0.4690159091 + 0.3875694076):
		output += 'c'
	else:
		output += 'd'
print(output)